<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="com.flow.snesde.tileeditor.TileEditor"
            contributorClass="org.eclipse.emf.edit.ui.action.EditingDomainActionBarContributor"
            default="true"
            extensions="tile"
            icon="graphics/icons/16/doc_tileset.png"
            id="com.flow.snesde.tileeditor.TileEditor"
            name="Tileset Editor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.core.contenttype.contentTypes">
      <file-association
            content-type="com.flow.snesde.editors.tileeditor.tilesettype"
            file-extensions="tile">
      </file-association>
   </extension>
</plugin>
